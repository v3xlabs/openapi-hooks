{
  "name": "openapi-hooks",
  "version": "0.0.1-beta.2",
  "description": "Magical fetch inference for OpenAPI with React Query support",
  "main": "./dist/index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsup src/index.ts",
